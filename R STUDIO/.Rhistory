# Dati di esempio (x, y)
x <- c(1, 4, 5, 6, 7,8,9,10,11)  # Coordinate x
y <- c(0, 0, 0.5, 1,1,0.75,0.50,0.25,0) # Coordinate y
# Creazione del dataframe con i dati
data <- data.frame(x, y)
# Interpolazione curva quadratica
model <- lm(y ~ poly(x, 2, raw = TRUE), data = data)
# Visualizzazione dei coefficienti
coefficients <- coef(model)
a <- coefficients[1] # Termine costante
b <- coefficients[2] # Coefficiente lineare
c <- coefficients[3] # Coefficiente quadratico
# Generazione dei punti interpolati
x_interpolated <- seq(min(x), max(x), length.out = 100) # Range di x per l'interpolazione
y_interpolated <- a + b * x_interpolated + c * x_interpolated^2 # Calcolo dei valori y interpolati
# Plot dei punti dati e della curva interpolata
plot(x, y, xlim = c(min(x), max(x)), ylim = c(min(y), max(y)), pch = 19, col = "red", xlab = "x", ylab = "y")
lines(x_interpolated, y_interpolated, col = "blue", lwd = 2)
legend("topright", legend = "Curva interpolata", col = "blue", lty = 1, lwd = 2)
# Plot dei punti dati e della curva interpolata
plot(x, y, xlim = c(min(x), max(x)), ylim = c(min(y), max(y)), pch = 19, col = "red", xlab = "x", ylab = "y")
plot(x, y, xlim = c(min(x), max(x)), ylim = c(min(y), max(y)), pch = 19, col = "red", xlab = "x", ylab = "y")
lines(x_interpolated, y_interpolated, col = "blue", lwd = 2)
legend("topright", legend = "Curva interpolata", col = "blue", lty = 1, lwd = 2)
